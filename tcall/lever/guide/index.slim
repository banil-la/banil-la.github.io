								td 0.3(300ms)
							tr
								th Interpolator
								td Accelerate
					.table-divide.width50
						p.table-divide-title Allow
						table
							tr
								th.title colspan="2" Icn
							tr
								th Attribute
								td Color
							tr
								th From
								td #FF0AC46A
							tr
								th To
								td #FFFFFFFF
							tr
								th Duration
								td 0.3(300ms)
							tr
								th Interpolator
								td Linear
						table
							tr
								th.title Circle
							tr
								th Attribute
								td Color
							tr
								th From
								td #000AC46A
							tr
								th To
								td #FF0AC46A
							tr
								th Duration
								td 0.3(300ms)
							tr
								th Interpolator
								td Accelerate
					table
						p.table-divide-title Common
						tr
							th.title colspan="2" Label
						tr
							th Attribute
							td Opacity
						tr
							th From
							td 1
						tr
							th To
							td 0
						tr
							th Duration
							td 0.15(150ms)
						tr
							th Interpolator
							td Decelerate

					table
						tr
							th.title &nbsp;
						tr
							th Attribute
							td Y
						tr
							th From
							td 0(Default)
						tr
							th To
							td -34
						tr
							th Duration
							td 0.3(300ms)
						tr
							th Interpolator
							td Decelerate
