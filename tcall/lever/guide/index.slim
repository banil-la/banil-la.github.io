							source src="mov/icncirclebtn.mov" type="video/mp4"
				.table-wrapper
					.table-divide.width50	
						p.table-divide-title Deny
						table
							tr
								th.title colspan="2" Icn
							tr
								th Attribute
								td Color
							tr
								th From
								td #FFF44A4D
							tr
								th To
								td #FFFFFFFFF
							tr
								th Duration
								td 0.3(300ms)
							tr
								th Interpolator
								td Linear
						table
							tr
								th.title Circle
							tr
								th Attribute
								td Color
							tr
								th From
								td #00F44A4D
							tr
								th To
								td #FFF44A4D
							tr
