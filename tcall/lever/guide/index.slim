html5
	head
		link rel="stylesheet" type="text/css" href="style.css"
		meta charset="UTF-8"
		meta name="T전화 수신레버 Intearction Guide"
		title T전화 수신레버 Interaction Guide

	body
		header
			p.title Interaction Guide
			p.desc T전화 수신레버 for Android

		section.title
			p.title 0. Interation Elements & Standard
			.contents
				.contents-wrapper
					.img-wrapper.width50
						img.common-img src="img/elements.png" 
					.img-wrapper.width50
						img.common-img src="img/z.png" 
		section.high-height
			p.title 1. Animating Icn, Circle, Label
			p.s-title State: Pressed -> Released
			.contents
				.contents-wrapper
					.img-wrapper.width70
						img.common-img src="img/btn.png" 
					.video-wrapper.width30
